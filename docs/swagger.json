                },
                "createdAt": {
                    "type": "string"
                },
                "id": {
                    "type": "string"
                },
                "isOpened": {
                    "type": "boolean"
                },
                "isStarred": {
                    "type": "boolean"
                },
                "messageText": {
                    "type": "string"
                },
                "ownerUsername": {
                    "type": "string"
                },
                "publicId": {
                    "type": "string"
                }
            }
        },
        "models.MessageMarkAsRead": {
            "type": "object",
            "properties": {
                "isStarred": {
                    "type": "boolean"
                }
            }
        },
        "models.TextMessageRequestSwagger": {
            "type": "object",
            "properties": {
                "messageText": {
                    "type": "string"
                },
                "ownerUsername": {
                    "type": "string"
                }
            }
        },
        "models.UserRequestDTO": {
            "type": "object",
