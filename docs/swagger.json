                    "type": "string"
                },
                "username": {
                    "type": "string"
                }
            }
        },
        "utils.APIResponse": {
            "type": "object",
            "properties": {
                "data": {
                    "description": "payload (optional)"
                },
                "error_code": {
                    "description": "internal or business error",
                    "type": "integer"
                },
                "message": {
                    "description": "readable human message",
                    "type": "string"
                },
                "success": {
                    "description": "\"success\" or \"error\"",
                    "type": "boolean"
                }
            }
